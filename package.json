{
  "name": "@ns3777k/mongoose-migrate",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "dependencies": {
    "mongoose": "^5.5.2"
  },
  "bin": {
    "migrate": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "eslint": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "yargs": "^13.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "scripts": {
    "setup": "npm ci",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint src __test__",
    "lint:eslint:fix": "eslint src __test__ --fix",
    "lint:prettier:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint:prettier:write": "prettier --write \"{src,test}/**/*.js\"",
    "build": "webpack && chmod +x dist/main.js",
    "test": "npm run lint && jest --ci",
    "test:coverage": "npm test -- --coverage"
  },
